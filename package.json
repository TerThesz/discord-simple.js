{
  "name": "discord-simple.js",
  "version": "1.3.5",
  "description": "A simplified Discord.js library for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "DEV=true nodemon --exec ts-node test/index.ts",
    "merge": "git checkout staging && git pull origin dev && git push && git checkout dev",
    "publish_package": "yarn build && npm publish",
    "build": "tsc",
    "postversion": "git push && yarn merge"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerThesz/discord-simple.js"
  },
  "keywords": [
    "discord",
    "simple",
    "bot"
  ],
  "author": "TerThesz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TerThesz/discord-simple.js/issues"
  },
  "homepage": "https://github.com/TerThesz/discord-simple.js#readme",
  "packageManager": "yarn@3.1.1",
  "devDependencies": {
    "@types/node": "^17.0.9",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
